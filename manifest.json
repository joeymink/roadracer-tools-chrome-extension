{
  "manifest_version": 2,

  "name": "Roadracer Results",
  "description": "Enhances motolaptimes.com to help racers evaluate their performance",
  "version": "0.1",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "http://motolaptimes.com/",
    "http://www.motolaptimes.com/"
  ],

  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
